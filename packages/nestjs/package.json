{
  "name": "@weave/nestjs",
  "version": "1.0.0",
  "description": "NestJS integration for Weave AI framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./modules": {
      "import": "./dist/modules/index.js",
      "require": "./dist/modules/index.cjs"
    },
    "./decorators": {
      "import": "./dist/decorators/index.js",
      "require": "./dist/decorators/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/modules/index.ts src/decorators/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest"
  },
  "dependencies": {
    "@weave/core": "1.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "tsup": "^8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
