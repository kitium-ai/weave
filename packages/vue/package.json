{
  "name": "@weave/vue",
  "version": "1.0.0",
  "description": "Vue 3 composables and components for Weave AI framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./composables": {
      "import": "./dist/composables/index.js",
      "require": "./dist/composables/index.cjs"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/composables/index.ts src/components/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest"
  },
  "dependencies": {
    "@weave/core": "1.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vue": "^3.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
