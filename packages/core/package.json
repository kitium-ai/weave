{
  "name": "@weaveai/core",
  "version": "1.1.0",
  "description": "Weave core framework - framework-agnostic AI operations",
  "license": "Apache-2.0",
  "author": "KitiumAI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./operations": {
      "types": "./dist/types/operations/index.d.ts",
      "import": "./dist/esm/operations/index.js",
      "require": "./dist/cjs/operations/index.js"
    },
    "./providers": {
      "types": "./dist/types/providers/index.d.ts",
      "import": "./dist/esm/providers/index.js",
      "require": "./dist/cjs/providers/index.js"
    },
    "./tools": {
      "types": "./dist/types/tools/index.d.ts",
      "import": "./dist/esm/tools/index.js",
      "require": "./dist/cjs/tools/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --sourcemap",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist coverage .turbo"
  },
  "dependencies": {
    "@weaveai/shared": "1.1.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "weave",
    "core",
    "framework",
    "agnostic",
    "operations",
    "providers",
    "tools",
    "types",
    "weaveai",
    "Kitium",
    "KitiumAI",
    "ai",
    "llm",
    "openai",
    "anthropic",
    "framework-agnostic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/weave.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kitium-ai/weave/issues"
  },
  "homepage": "https://github.com/kitium-ai/weave#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
