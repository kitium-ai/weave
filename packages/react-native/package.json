{
  "name": "@weaveai/react-native",
  "version": "1.1.0",
  "description": "React Native integration for Weave AI framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/hooks/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest"
  },
  "dependencies": {
    "@weaveai/core": "1.1.0",
    "@weaveai/shared": "1.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native": "^0.70.0 || ^0.71.0 || ^0.72.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "react": "^18.0.0",
    "react-native": "^0.71.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "weave",
    "weaveai",
    "react",
    "react-native",
    "hooks",
    "native",
    "kitium",
    "kitiumai",
    "ai",
    "llm",
    "typescript",
    "javascript",
    "openai",
    "anthropic",
    "google"
  ]
}
