{
  "name": "@weave/nodejs",
  "version": "1.0.0",
  "description": "Node.js integration for Weave AI framework",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest"
  },
  "dependencies": {
    "@weave/core": "1.0.0"
  },
  "devDependencies": {
    "express": "^4.18.0",
    "tsup": "^8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
