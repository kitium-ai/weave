{
  "name": "create-weave-app",
  "version": "1.0.0",
  "description": "Interactive CLI to scaffold a new Weave AI application in seconds",
  "type": "module",
  "bin": {
    "create-weave-app": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --outDir dist",
    "dev": "tsup src/index.ts --watch --format esm",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "weave",
    "ai",
    "cli",
    "scaffold",
    "generator",
    "typescript",
    "react",
    "vue",
    "svelte",
    "angular"
  ],
  "dependencies": {
    "prompts": "^2.4.2",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "fs-extra": "^11.2.0",
    "path-exists": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
